jQuery(document).ready(function($){
	jQuery("#kwfield").keyup(function(){
		var thisval = jQuery(this).val();
		if( thisval != ''){
			var this_parent = jQuery(this).parent();
			var loader = "https://worldscreen.com/wp-content/themes/sahifa-child/images/loader.gif";
			jQuery(this_parent).append('<img src="'+loader+'" width="40px" id="kwrd_loader">');
			var url = "https://worldscreen.com/wp-admin/admin-ajax.php"
			var data = {
				action: 'ws_auto_tag',
				tag: thisval
				};
			jQuery.post(url, data, function( response ) {
				jQuery('#kwrd_loader').hide();
				jQuery(this_parent).find('#kwrd_loader').remove();
				if(response == ''){
					$('#auto-res').hide();
				}else
				{
					var spl = response.split(",");
					$('#auto-res li').remove();
					$('#auto-res').show();
					jQuery(spl).each(function(item, val){
						$('#auto-res').append('<li class="as_item">'+val+'</li>');
					});
					jQuery('.as_item').click(function(){
						var tag = $(this).text();
						$('#kwfield').val(tag);
						$('#auto-res').hide();
					});
				}
			});
		}else{
			$('#auto-res li').remove();
			$('#auto-res').hide();
		}
	});

});


</script>
<script>

    jQuery(document).ready(function() {

// Have the first and last li's set to a variable
var $first = jQuery('li:first', 'ul'),
    $last = jQuery('li:last', 'ul');


// Set time intervall

jQuery("#next").click(function () {

    var $next,
        $selected = jQuery(".playlist_sidebar .selected");
    // get the selected item
    // If next li is empty , get the first
    $next = $selected.next('li').length ? $selected.next('li') : $first;

	if (jQuery('.playlist_sidebar ul li:last-child').hasClass('selected')) {
		$next.addClass('selected').fadeIn('fast');
		$selected.removeClass("selected").fadeOut('fast');
		jQuery('.playlist_sidebar ul li:first-child').addClass('selected');
	}else{
		$next.addClass('selected').fadeIn('fast');
		$selected.removeClass("selected").fadeOut('fast');
	}

});

jQuery("#prev").click(function () {
    var $prev,
        $selected = jQuery(".playlist_sidebar .selected");
    // get the selected item
    // If prev li is empty , get the last
    $prev = $selected.prev('li').length ? $selected.prev('li') : $last;
	if (jQuery('.playlist_sidebar ul li:first-child').hasClass('selected')) {
		$next.addClass('selected').fadeIn('fast');
		$selected.removeClass("selected").fadeOut('fast');
		jQuery('.playlist_sidebar ul li:last-child').addClass('selected');
	}else{
		$prev.addClass('selected').fadeIn('fast');
		$selected.removeClass("selected").fadeOut('fast');
	}
    //$selected.removeClass("selected").fadeOut('fast');
    //$prev.addClass('selected').fadeIn('fast');
});

});

